{
  "name": "consensys-test-api",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn -- src/app.ts",
    "start": "ts-node src/app.ts",
    "test": "jest",
    "test:cover": "jest --coverage",
    "test:ci": "jest --config ./jest.config.ts --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --watchAll=false",
    "build": "rm -rf ./dist && tsc --project tsconfig.build.json",
    "migration:generate": "npm run build && typeorm -f migrationsormconfig.js migration:generate",
    "migration:run": "npm run build && typeorm -f migrationsormconfig.js migration:run",
    "migration:revert": "npm run build && typeorm -f migrationsormconfig.js migration:revert",
    "seed:run": "npm run build && ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "author": "Edoardo Foco",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^3.8.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "jwks-rsa": "^2.1.4",
    "pg": "^8.7.3",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.7",
    "typeorm-seeding": "^1.6.1",
    "typeorm-typedi-extensions": "^0.4.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "jest": "^28.1.1",
    "jsonwebtoken": "^8.5.1",
    "moq.ts": "^9.0.2",
    "ts-jest": "^28.0.4",
    "ts-node-dev": "^2.0.0"
  }
}
